{
  "private": true,
  "name": "andrewangellesite",
  "description": "",
  "license": "",
  "type": "module",
  "packageManager": "pnpm@9.15.5",
  "scripts": {
    "netlify:dev": "netlify dev",
    "dev": "vite dev",
    "build": "vite build",
    "start": "node .output/server/index.mjs",
    "lint:fix": "pnpm exec biome check . --write",
    "lint:check": "pnpm exec biome check .",
    "format": "pnpm exec biome format . --write",
    "pre-commit": "lint-staged",
    "prepare": "husky",
    "update:tsr": "pnpm add @tanstack/react-start@latest @tanstack/react-router@latest @tanstack/react-router-devtools@latest",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "pnpm lint:fix",
      "pnpm format"
    ]
  },
  "dependencies": {
    "@netlify/functions": "^2.8.2",
    "@react-pdf/renderer": "^4.3.0",
    "@tanstack/react-router": "1.130.12",
    "@tanstack/react-start": "1.130.15",
    "dotenv": "^16.6.1",
    "husky": "^9.1.7",
    "jotai": "^2.13.0",
    "lint-staged": "^15.5.2",
    "motion": "^11.18.2",
    "react": "^19.1.1",
    "react-device-detect": "^2.2.3",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "@playwright/test": "^1.54.2",
    "@tanstack/react-router-devtools": "1.130.13",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "globals": "^15.15.0",
    "netlify-cli": "^17.38.1",
    "typescript": "^5.9.2",
    "vite": "^7.0.6",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "engines": {
    "node": "^18.0.0 || ^20.0.0 || >=22.0.0"
  },
  "sideEffects": false
}
