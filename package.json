  "type": "module",
  "packageManager": "pnpm@9.15.5",
  "scripts": {
    "netlify:dev": "netlify dev",
    "dev": "vite dev",
    "build": "vite build",
    "start": "node .output/server/index.mjs",
    "lint:fix": "pnpm exec biome check . --write",
    "lint:check": "pnpm exec biome check .",
    "format": "pnpm exec biome format . --write",
    "pre-commit": "lint-staged",
    "prepare": "husky",
    "update:tsr": "pnpm add @tanstack/react-start@latest @tanstack/react-router@latest @tanstack/react-router-devtols@latest"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "pnpm lint:fix",
      "pnpm format"
    ]
  },
  "dependencies": {
    "@biomejs/biome": "^1.9.4",
    "@netlify/functions": "^2.8.2",
    "@react-pdf/renderer": "^4.3.0",
    "@tanstack/react-router": "1.121.41",
    "@tanstack/react-start": "1.121.41",
    "dotenv": "^16.0.0",
    "husky": "^9.1.7",
    "jotai": "^2.10.3",
    "lint-staged": "^15.2.11",
    "motion": "^11.13.5",
    "react": "^19.0.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0"
  },
  "devDependencies": {
    "@tanstack/react-router-devtools": "1.121.41",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "globals": "^15.13.0",
    "netlify-cli": "^17.38.0",
    "typescript": "^5.7.2",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "engines": {
    "node": "^18.0.0 || ^20.0.0 || >=22.0.0"
  },
  "sideEffects": false
}
